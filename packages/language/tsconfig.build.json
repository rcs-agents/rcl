{
    "extends": "../../tsconfig.options.json",
    "compilerOptions": {
      "rootDir": "./src",
      "outDir": "lib",
      "tsBuildInfoFile": "../../.moon/cache/tsc/language.tsbuildinfo",
      "moduleResolution": "nodenext",
      "paths": {
        "#src": ["./src/index.ts"],
        "#src/*": [
          "./src/*/index.ts",
          "./src/*.ts",
          "./src/*.js",
          "./src/*"
        ],
        "#tests/*": [
          "./tests/*.ts",
          "./tests/*.js",
          "./tests/*"
        ]
      },
    },
    "include": [
      "src/**/*.ts"
    ]
  }
